{
  "name": "HiP-CmsAngularApp",
  "version": "1.0.0",
  "description": "Client Part of the History in Paderborn app, written with Angular 2",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "docker-build": "docker build -t hip-cms-angular .",
    "docker": "npm run docker-build && docker run -it --rm -p 8080:8080 hip-cms-angular",
    "e2e": "tsc && concurrently --kill-others \"http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install; npm run sass",
    "serve": "angular-http-server",
    "test": "tsc && karma start karma.conf.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p ./",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "doc": "typedoc --out \"docs/\" app/",
    "cleanup": "rm -R node_modules typings; npm install",
    "sass": "node-sass hip-theme.scss hip-theme.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "@angular/upgrade": "2.1.2",
    "@angular/material": "2.0.0-alpha.10",
    "angular-http-server": "1.0.7",
    "angular2-in-memory-web-api": "0.0.21",
    "angular2-jwt": "0.1.25",
    "angular2-toaster": "^1.0.1",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "ng2-pagination": "1.0.0",
    "ng2-tag-input": "0.4.4",
    "ng2-translate": "4.0.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.41",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.1.2",
    "@angular/platform-server": "2.1.2",
    "bootstrap": "^3.3.6",
    "canonical-path": "0.0.2",
    "codelyzer": "^1.0.0-beta.0",
    "concurrently": "^3.1.0",
    "jasmine-core": "~2.5.2",
    "jquery": "^3.1.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "~1.0.0",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "lite-server": "^2.2.0",
    "lodash": "4.17.0",
    "node-sass": "3.13.0",
    "protractor": "4.0.11",
    "rimraf": "^2.5.2",
    "tslint": "^3.15.1",
    "typedoc": "^0.5.1",
    "typescript": "2.0.9",
    "typings": "^2.0.0"
  },
  "repository": {}
}
