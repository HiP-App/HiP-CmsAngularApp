{
  "name": "HiP-CmsAngularApp",
  "version": "1.0.0",
  "description": "Client Part of the History in Paderborn app, written with Angular 2",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "docker-build": "docker build -t hip-cms-angular .",
    "docker": "npm run docker-build && docker run -it --rm -p 8080:8080 hip-cms-angular",
    "e2e": "tsc && concurrently -n \"HTTP,e2e\" -p name -s first -k \"angular-http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install && npm run sass",
    "serve": "angular-http-server",
    "test": "tsc && karma start karma.conf.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p ./",
    "typings": "typings install",
    "webdriver:update": "webdriver-manager update --versions.standalone 3.2.0",
    "doc": "typedoc --ignoreCompilerErrors --out \"docs/\" app/",
    "clean": "rimraf hip-theme.css typings node_modules || exit 0",
    "clean-install": "npm run clean && npm install",
    "sass": "node-sass hip-theme.scss hip-theme.css",
    "ng2inline": "ng2inline --outDir=dist \"app/**/*.ts\"",
    "compile": "cd dist && tsc && browserify --ignore-missing --fast -s main main.js > bundle.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build_prod": "npm run ng2inline && npm run compile && npm run minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "4.1.1",
    "@angular/common": "4.1.1",
    "@angular/compiler": "4.1.1",
    "@angular/core": "4.1.1",
    "@angular/forms": "4.1.1",
    "@angular/http": "4.1.1",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "4.1.1",
    "@angular/platform-browser-dynamic": "4.1.1",
    "@angular/router": "4.1.1",
    "@angular/upgrade": "4.1.1",
    "@types/hammerjs": "2.0.34",
    "@types/lodash": "4.14.56",
    "angular-http-server": "1.0.7",
    "angular2-jwt": "0.2.0",
    "angular2-toaster": "3.0.1",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "dragula": "3.7.2",
    "hammerjs": "2.0.8",
    "ng2-dragula": "1.3.1",
    "ng2-material-dropdown": "0.7.7",
    "ng2-tag-input": "1.2.2",
    "ng2-translate": "5.0.0",
    "ngx-pagination": "3.0.0",
    "rxjs": "5.3.1",
    "systemjs": "0.20.12",
    "zone.js": "0.8.10"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.1.2",
    "@angular/platform-server": "4.1.1",
    "@types/jasmine": "2.5.45",
    "browserify": "14.3.0",
    "canonical-path": "0.0.2",
    "codelyzer": "3.0.1",
    "concurrently": "3.4.0",
    "jasmine-core": "2.6.1",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.1.0",
    "karma-cli": "1.0.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-htmlfile-reporter": "0.3.5",
    "karma-jasmine": "1.1.0",
    "lite-server": "2.3.0",
    "lodash": "4.17.4",
    "ng2-inline-template": "0.1.1",
    "node-sass": "4.5.2",
    "protractor": "5.1.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "systemjs-plugin-text": "0.0.9",
    "tslint": "5.2.0",
    "typedoc": "0.6.0",
    "typescript": "2.3.2",
    "typings": "2.1.1"
  },
  "repository": {}
}
