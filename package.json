{
  "name": "HiP-CmsAngularApp",
  "version": "1.0.0",
  "description": "Client Part of the History in Paderborn app, written with Angular 2",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "docker-build": "docker build -t hip-cms-angular .",
    "docker": "npm run docker-build && docker run -it --rm -p 8080:8080 hip-cms-angular",
    "e2e": "tsc && concurrently -n \"HTTP,e2e\" -p name -s first -k \"angular-http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "npm run sass",
    "serve": "angular-http-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p ./",
    "webdriver:update": "webdriver-manager update --versions.standalone 3.2.0",
    "doc": "typedoc --ignoreCompilerErrors --out \"docs/\" app/",
    "clean": "rimraf hip-theme.css node_modules || exit 0",
    "clean-install": "npm run clean && npm install",
    "sass": "node-sass hip-theme.scss hip-theme.css",
    "ng2inline": "ng2inline --outDir=dist \"app/**/*.ts\"",
    "compile": "cd dist && tsc && browserify --ignore-missing --fast -s main main.js > bundle.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build_prod": "npm run ng2inline && npm run compile && npm run minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@agm/core": "1.0.0-beta.0",
    "@angular/animations": "4.3.6",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@angular/router": "4.3.6",
    "@types/core-js": "0.9.42",
    "angular-http-server": "1.3.1",
    "angular2-jwt": "0.2.3",
    "angular2-toaster": "4.0.1",
    "auth0-js": "8.9.3",
    "core-js": "2.5.0",
    "dragula": "3.7.2",
    "ng2-dragula": "1.5.0",
    "ng2-material-dropdown": "0.7.7",
    "ng2-tag-input": "1.2.2",
    "ng2-translate": "5.0.0",
    "ngx-pagination": "3.0.1",
    "rxjs": "5.4.3",
    "systemjs": "0.20.18",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.3.6",
    "@types/auth0-js": "8.6.3",
    "@types/jasmine": "2.5.45",
    "@types/lodash": "4.14.56",
    "browserify": "14.4.0",
    "codelyzer": "3.1.2",
    "concurrently": "3.5.0",
    "jasmine-core": "2.7.0",
    "lite-server": "2.3.0",
    "lodash": "4.17.4",
    "ng2-inline-template": "0.1.1",
    "node-sass": "4.5.3",
    "protractor": "5.1.2",
    "rimraf": "2.6.1",
    "tslint": "5.6.0",
    "typedoc": "0.8.0",
    "typescript": "2.4.2"
  },
  "repository": {}
}
