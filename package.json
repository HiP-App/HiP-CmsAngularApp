{
  "name": "HiP-CmsAngularApp",
  "version": "1.0.0",
  "description": "Client Part of the History in Paderborn app, written with Angular 2",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "docker-build": "docker build -t hip-cms-angular .",
    "docker": "npm run docker-build && docker run -it --rm -p 8080:8080 hip-cms-angular",
    "e2e": "tsc && concurrently --kill-others \"http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install && npm run sass",
    "serve": "angular-http-server",
    "test": "tsc && karma start karma.conf.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p ./",
    "typings": "typings install",
    "webdriver:update": "webdriver-manager update",
    "doc": "typedoc --out \"docs/\" app/",
    "clean": "rimraf hip-theme.css typings node_modules || exit 0",
    "clean-install": "npm run clean && npm install",
    "sass": "node-sass hip-theme.scss hip-theme.css",
    "ng2inline": "ng2inline --outDir=dist \"app/**/*.ts\"",
    "compile": "cd dist && tsc && browserify --ignore-missing --fast -s main main.js > bundle.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build_prod": "npm run ng2inline && npm run compile && npm run minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/material": "2.0.0-alpha.10",
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "@angular/upgrade": "2.4.1",
    "angular-http-server": "1.0.7",
    "angular2-color-picker": "1.3.0",
    "angular2-jwt": "0.1.27",
    "angular2-toaster": "1.1.0",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "ng2-material-dropdown": "0.5.5",
    "ng2-pagination": "1.0.1",
    "ng2-tag-input": "0.4.9",
    "ng2-translate": "5.0.0",
    "rxjs": "5.0.2",
    "systemjs": "0.19.41",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.1",
    "@angular/platform-server": "2.4.1",
    "angular-inject-templates": "0.1.2",
    "browserify": "13.1.1",
    "canonical-path": "0.0.2",
    "codelyzer": "2.0.0-beta.4",
    "concurrently": "3.1.0",
    "jasmine-core": "2.5.2",
    "jquery": "3.1.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-htmlfile-reporter": "0.3.4",
    "karma-jasmine": "1.1.0",
    "lite-server": "2.2.2",
    "lodash": "4.17.3",
    "ng2-inline-template": "0.1.1",
    "node-sass": "4.1.1",
    "postcss-loader": "1.2.1",
    "protractor": "4.0.14",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "systemjs-plugin-text": "0.0.9",
    "tslint": "4.2.0",
    "typedoc": "0.5.1",
    "typescript": "2.1.4",
    "typings": "2.1.0",
    "webpack": "1.14.0"
  },
  "repository": {}
}
